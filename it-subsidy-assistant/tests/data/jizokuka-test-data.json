  "questionFlows": [
    {
      "categoryId": "通常枠",
      "questions": [
        {
          "id": "q1",
          "text": "あなたの事業形態は？",
          "type": "radio",
          "options": [
            {"value": "individual", "text": "個人事業主"},
            {"value": "corporation", "text": "法人"}
          ]
        },
        {
          "id": "q2",
          "text": "従業員数は？（代表者除く）",
          "type": "number",
          "min": 0,
          "max": 50,
          "industryCheck": true
        },
        {
          "id": "q3",
          "text": "主な取り組み内容は？",
          "type": "radio",
          "options": [
            {"value": "sales_channel", "text": "販路開拓", "points": 3},
            {"value": "productivity", "text": "生産性向上", "points": 3},
            {"value": "both", "text": "両方", "points": 4}
          ],
          "threshold": 3
        }
      ],
      "judgmentLogic": "employeeCheck(q1, q2) AND q3 >= 3"
    },
    {
      "categoryId": "賃金引上げ枠",
      "questions": [
        {
          "id": "q1",
          "text": "事業場内最低賃金の引上げ予定額は？",
          "type": "number",
          "min": 30,
          "max": 200,
          "unit": "円",
          "threshold": 30
        },
        {
          "id": "q2",
          "text": "赤字事業者ですか？",
          "type": "radio",
          "options": [
            {"value": "yes", "text": "はい（補助率3/4）"},
            {"value": "no", "text": "いいえ（補助率2/3）"}
          ]
        }
      ],
      "judgmentLogic": "q1 >= 30"
    },
    {
      "categoryId": "卒業枠",
      "questions": [
        {
          "id": "q1",
          "text": "小規模事業者卒業の具体的計画はありますか？",
          "type": "radio",
          "options": [
            {"value": "yes", "text": "あり", "points": 3},
            {"value": "planning", "text": "検討中", "points": 2},
            {"value": "no", "text": "なし", "points": 1}
          ],
          "threshold": 3
        }
      ],
      "judgmentLogic": "q1 >= 3"
    },
    {
      "categoryId": "後継者支援枠",
      "questions": [
        {
          "id": "q1",
          "text": "代表者の年齢は？",
          "type": "number",
          "min": 18,
          "max": 100,
          "threshold": 60
        },
        {
          "id": "q2",
          "text": "後継者は決定していますか？",
          "type": "radio",
          "options": [
            {"value": "decided", "text": "決定済み", "points": 3},
            {"value": "candidate", "text": "候補者あり", "points": 2},
            {"value": "undecided", "text": "未定", "points": 1}
          ],
          "threshold": 3
        }
      ],
      "judgmentLogic": "q1 >= 60 AND q2 >= 3"
    },
    {
      "categoryId": "創業枠",
      "questions": [
        {
          "id": "q1",
          "text": "産業競争力強化法に基づく創業支援を受けましたか？",
          "type": "radio",
          "options": [
            {"value": "received", "text": "受けた", "points": 3},
            {"value": "applying", "text": "申請中", "points": 2},
            {"value": "not_received", "text": "受けていない", "points": 1}
          ],
          "threshold": 3
        },
        {
          "id": "q2",
          "text": "事業開始からの期間は？",
          "type": "radio",
          "options": [
            {"value": "within1year", "text": "1年未満", "points": 3},
            {"value": "within3years", "text": "3年未満", "points": 2},
            {"value": "over3years", "text": "3年以上", "points": 1}
          ],
          "threshold": 2
        }
      ],
      "judgmentLogic": "q1 >= 3 AND q2 >= 2"
    }
  ],
